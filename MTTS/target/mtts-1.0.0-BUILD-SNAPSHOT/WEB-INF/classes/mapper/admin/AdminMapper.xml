<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mju.mtts.dao.admin.AdminInfoDao">
	<sql id="default-condition">
		<where>
            <if test="adminId != null">
                AND admin_id = #{adminId}
            </if>
            <if test="adminPw != null">
                AND admin_pw = #{adminPw}
            </if>
		</where>
	</sql>
	
	<select id="selectAll" resultType="adminInfo">
		SELECT
			 * 
		FROM	admin.admin
		<if test="adminIdList != null">
			WHERE seq = #{seq}
		</if>
	</select>
	
</mapper>