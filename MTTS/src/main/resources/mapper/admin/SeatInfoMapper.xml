<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mju.mtts.dao.movie.SeatInfoDao">

	<select id="selectAll" resultType="seatInfo">
		SELECT
			SEAT_NUMBER as 'seatNumber'

		FROM
			SEAT_INFO
		WHERE
			SHOWTIME_SEQ = #{0}
		AND
			SEAT_RESERV_TON = 0
	</select>
</mapper>